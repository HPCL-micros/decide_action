#include "micros_flock.cpp"

int main(int argc, char** argv)
{
    ros::init(argc,argv,"turtlebot_roll_node");
    micros_flock::FlockingAlgorithm fa;
    geometry_msgs:: Twist msg;
    ros::Rate loop_rate(20);
    while(ros::ok())
    {
        ros::spinOnce();
        fa.run(msg);
        loop_rate.sleep();
    }
}
